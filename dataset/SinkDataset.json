{
	"name": "SinkDataset",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureStorageLinkedService",
			"type": "LinkedServiceReference"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@CONCAT('Incremental-', pipeline().RunId, '.txt')",
					"type": "Expression"
				},
				"folderPath": "data",
				"container": "root"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "R_Number",
				"type": "String"
			},
			{
				"name": "RepairNumber",
				"type": "String"
			},
			{
				"name": "Part#",
				"type": "String"
			},
			{
				"name": "Serial#",
				"type": "String"
			},
			{
				"name": "Status",
				"type": "String"
			}
		]
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}